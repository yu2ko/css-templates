<div class='page'>
  <div class='page_content'>
	<div class='page_block'>
		<header class='header row g-0'>
			<div class='col-3'>
				{{> logo}}
			</div>
			
			<nav class='header_nav col-9'>
				{{!-- {{> nav list=header isVertical=true}} --}}
				<div class="header_nav_right">
					{{> nav list=header}}
				</div>
			</nav>
		</header>
		{{!-- <hr class="divider-hr"> --}}
		{{> divider }}
	</div>
	
	<div class="page_block">
		{{> hero_t12 title=hero.title text=hero.text}}
	</div>

	{{!-- <div class="page_block">
		{{> hero title="Hello!" text="Hello world!"}}
	</div> --}}

	<div class="page_block">
		<main class="row g-0 plans">
			{{#each plans}}
				<div class="col plans_card">
					{{> card_practice1 data=this}}
					{{!-- <div class="card">
						<div class="card-header">
							{{name}}
						</div>
						<div class="card-body">
							<div class="card_block">
								<span class="card_price_amount">
									${{price}}
								</span>
								<span class="card_price_duration">
									/ mo
								</span>
								<div class="card_block">
									{{#each features}}
									<span class="card_description_item">{{this}}</span>
									{{/each}}
								</div>
								<div class="card_block">
									<button class="btn {{#if @first}}btn-outline-primary{{else}}btn-primary{{/if}} card_button">{{button}}</button>
								</div>
							</div>
						</div>
					</div> --}}
				</div>
			{{/each}}
		</main>
	</div>

	<div class="page_block">
		{{!-- <hr class="divider-hr"> --}}
		{{> divider }}
		<footer class="row footer">
			<div class="col-3">
				{{> logo}}
				<span class="footer_copyright">Copyright 2023-2024</span>
			</div>
			
				{{#each footer}}
				<div class="col-3">
				<span class="footer_title">{{title}}</span>
				{{> nav list=links isVertical=true}}
				</div>
				{{/each}}
			
		</footer>
	</div>
  </div>
</div>